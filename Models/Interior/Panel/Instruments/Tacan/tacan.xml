    <object-name>clic-vol-R</object-name>
  </animation>
  
  <animation>
    <rotate>
    <object-name>All-Vol</object-name>
    <property>?</property>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
